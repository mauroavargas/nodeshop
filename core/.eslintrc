{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "node"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
      "brace-style": "off",
      "arrow-body-style": "off",
      "comma-dangle": "off",
      "no-use-before-define": "off"
    }
}
